	0 -> 4 [label=1]
	8
	8 -> 6 [label=1]
	7
	7 -> 8 [label=0]
	5
	5 -> 7 [label=1]
	6
	6 -> 1 [label="ε"]
	9
	9 -> 10 [label="ε"]
	10
	10 -> 1 [label="ε"]
	11
	11 -> 5 [label="ε"]
	11 -> 9 [label="ε"]
	2
	2 -> 3 [label=0]
	3
	3 -> 2 [label="ε"]
	3 -> 11 [label="ε"]
	4
	4 -> 2 [label="ε"]
	4 -> 11 [label="ε"]
	1 [shape=doublecircle]
}
