digraph finite_state_machine {
	rankdir=LR
	node [shape=circle]
	start_mark [shape=point style=invis]
	6
	start_mark -> 6
	6 -> 4 [label="ε"]
	6 -> 7 [label="ε"]
	0
	0 -> 1 [label=a]
	1
	1 -> 5 [label="ε"]
	2
	2 -> 3 [label=b]
	3
	3 -> 5 [label="ε"]
	4
	4 -> 0 [label="ε"]
	4 -> 2 [label="ε"]
	5
	5 -> 4 [label="ε"]
	5 -> 7 [label="ε"]
	7 [shape=doublecircle]
}
